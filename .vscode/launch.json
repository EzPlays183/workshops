{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Current JavaScript File",
      "program": "${file}",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal"
    },
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Run Current Python File",
      "program": "${file}",
      "console": "integratedTerminal"
    }
  ]
}
